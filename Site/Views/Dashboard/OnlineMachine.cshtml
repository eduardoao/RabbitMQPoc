@model IEnumerable<Domain.Entities.AplicationDomain>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ServiceName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServiceDisplayName)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.MachiName)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.DateTimeUtc)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ServiceName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ServiceDisplayName)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.MachiName)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.DateTimeUtc)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
